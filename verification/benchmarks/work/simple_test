#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2009.vpi";
S_0x127004cd0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x127004e40 .scope module, "simple_tb" "simple_tb" 3 4;
 .timescale -9 -12;
v0x127004ff0_0 .var "clk", 0 0;
v0x116612110_0 .var "rst_n", 0 0;
v0x1166121c0_0 .var "test_data", 31 0;
v0x116612270_0 .var "test_valid", 0 0;
E_0x127004fb0 .event posedge, v0x127004ff0_0;
    .scope S_0x127004e40;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x127004ff0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x116612110_0, 0, 1;
    %pushi/vec4 305419896, 0, 32;
    %store/vec4 v0x1166121c0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x116612270_0, 0, 1;
    %end;
    .thread T_0, $init;
    .scope S_0x127004e40;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x127004ff0_0;
    %inv;
    %store/vec4 v0x127004ff0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x127004e40;
T_2 ;
    %vpi_call/w 3 20 "$display", "Starting simple RTL test..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x116612110_0, 0, 1;
    %pushi/vec4 10, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x127004fb0;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x116612110_0, 0, 1;
    %vpi_call/w 3 24 "$display", "Reset released" {0 0 0};
    %pushi/vec4 5, 0, 32;
T_2.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.3, 5;
    %jmp/1 T_2.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x127004fb0;
    %jmp T_2.2;
T_2.3 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x116612270_0, 0, 1;
    %wait E_0x127004fb0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x116612270_0, 0, 1;
    %pushi/vec4 20, 0, 32;
T_2.4 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.5, 5;
    %jmp/1 T_2.5, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x127004fb0;
    %jmp T_2.4;
T_2.5 ;
    %pop/vec4 1;
    %vpi_call/w 3 33 "$display", "Test completed successfully" {0 0 0};
    %vpi_call/w 3 34 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x127004e40;
T_3 ;
    %wait E_0x127004fb0;
    %load/vec4 v0x116612110_0;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_3.2, 9;
    %load/vec4 v0x116612270_0;
    %and;
T_3.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %vpi_call/w 3 40 "$display", "Time %0t: Test data = 0x%h", $time, v0x1166121c0_0 {0 0 0};
T_3.0 ;
    %jmp T_3;
    .thread T_3;
